{
  "cleanUrls": false,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/premiacoes-robos",
      "destination": "/premiacoes-robos.html"
    },
    {
      "source": "/sobre-first",
      "destination": "/sobre-first.html"
    },
    {
      "source": "/mentorias",
      "destination": "/mentorias.html"
    },
    {
      "source": "/patrocinio",
      "destination": "/patrocinio.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.(html|css|js|webp|otf|png|jpg|jpeg|svg|ico))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

